{
  "name": "react-jsgraph",
  "version": "2.0.0",
  "description": "React components integrating jsGraph",
  "homepage": "https://github.com/zakodium/react-jsgraph#readme",
  "files": [
    "lib",
    "lib-es6"
  ],
  "main": "lib/index.js",
  "keywords": [
    "react",
    "jsgraph",
    "graph",
    "chart"
  ],
  "module": "lib-es6/index.js",
  "sideEffect": false,
  "scripts": {
    "build-docs": "build-storybook -o docs",
    "compile": "babel src --out-dir lib --ignore __tests__ --quiet",
    "compile-es6": "cross-env BABEL_ENV=es6 babel src --out-dir lib-es6 --ignore __tests__ --quiet",
    "eslint": "eslint --cache src stories",
    "eslint-fix": "npm run eslint -- --fix",
    "prepare": "npm run compile && npm run compile-es6",
    "storybook": "start-storybook -p 6006",
    "test": "npm run eslint"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-info": "^5.1.10",
    "@storybook/addon-knobs": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-config-cheminfo": "^2.0.3",
    "eslint-config-cheminfo-react": "^3.0.4",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "repository": "git://github.com/zakodium/react-jsgraph.git",
  "license": "MIT",
  "dependencies": {
    "@nivo/core": "^0.59.1",
    "node-jsgraph": "^2.2.23"
  }
}
